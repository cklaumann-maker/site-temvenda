<?php
// Arquivo para forçar a exibição da página de treinamento in-company
// Acesse via: http://localhost:8080/treinamento-incompany

// Carregar o conteúdo do arquivo HTML do treinamento in-company
$treinamento_html_path = '/var/www/html/treinamento-incompany-corporativo.html';

if (file_exists($treinamento_html_path)) {
    echo file_get_contents($treinamento_html_path);
} else {
    echo "<h1>Erro: Arquivo do treinamento in-company não encontrado.</h1>";
}
?>
