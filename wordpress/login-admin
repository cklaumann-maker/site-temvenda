<?php
// Arquivo para forçar a exibição da página de login
// Acesse via: http://localhost:8080/login-admin

// Carregar o conteúdo do arquivo HTML do login
$login_html_path = '/var/www/html/login-admin.html';

if (file_exists($login_html_path)) {
    echo file_get_contents($login_html_path);
} else {
    echo "<h1>Erro: Arquivo de login não encontrado.</h1>";
}
?>

