<?php
// Arquivo para forçar a exibição da página de formação
// Acesse via: http://localhost:8080/formacao-lideres

// Carregar o conteúdo do arquivo HTML da formação
$formacao_html_path = '/var/www/html/formacao-lideres-corporativo.html';

if (file_exists($formacao_html_path)) {
    echo file_get_contents($formacao_html_path);
} else {
    echo "<h1>Erro: Arquivo da formação não encontrado.</h1>";
}
?>

